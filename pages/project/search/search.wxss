/* pages/project/search/search.wxss */
page{
  background: #fff;
}
.search_container{
  width: 750rpx;
  box-sizing: border-box;
  background: #fff;
  padding: 20rpx 30rpx;
  position: relative;
  border-top:1rpx solid rgb(230,230,230);
  border-bottom:1rpx solid rgb(230,230,230);
}
.search_container image{
  width: 35rpx;
  display: block;
  opacity: 0;
}
.search_container .active{
  opacity: 1;
}
.search_container view{
  width: 690rpx;
  box-sizing: border-box;
  border: none;
  background: #eee;
  height: 60rpx;
  border-radius: 60rpx;
  font-size: 28rpx;
  color:#282828;
  padding: 0 16rpx;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: center;
}
.search_container input{
  height: 40rpx;
  line-height: 40rpx;
  background: #eee;
  border:none;
  padding:0;
  width: 604rpx;
  margin:0 0 0 10rpx;
}
.search_su{
  background: #fff;
  padding: 50rpx 30rpx 0;
  width: 750rpx;
  box-sizing: border-box;
}
.search_title{
  color:rgb(102,102,102);
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 30rpx;
  position: relative;
}
.search_title image{
  position: absolute;
  width: 35rpx;
  height: 37rpx;
  display: block;
  right:0;
  top:50%;
  transform: translateY(-50%);
}
.key_container{
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-items: center;
  width: 690rpx;
}
.key_container view{
  height: 45rpx;
  line-height: 45rpx;
  font-size: 28rpx;
  color:rgb(102,102,102);
  background: #eee;
  padding: 0 10rpx;
  border-radius: 45rpx;
  margin-right: 19rpx;
  margin-bottom: 20rpx;
}

.result{
  width: 750rpx;
  font-size: 32rpx;
  color:rgb(102,102,102);

}
.result .strong{
  color:rgb(40,40,40);
}
.search_result{
  width: 750rpx;
  box-sizing: border-box;
  padding-left: 29rpx;
}
.result{
  padding: 30rpx 20rpx 30rpx 0;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  width: 721rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.result .rich{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}